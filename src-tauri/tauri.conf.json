{
  "productName": "FeatherSQL",
  "version": "0.1.0",
  "identifier": "com.feathersql.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "FeatherSQL",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "visible": true,
        "skipTaskbar": false,
        "focus": true,
        "center": true
      }
    ],
    "security": {
      "csp": null,
      "capabilities": [
        "main"
      ]
    }
  },
  "bundle": {
    "active": true,
    "targets": [
      "msi"
    ],
    "icon": [
      "icons/icon.ico"
    ],
    "windows": {
      "wix": {
        "language": "zh-CN"
      }
    }
  }
}
